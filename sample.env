# Runtime configuration for cc-proxy (devcontainer)
#
# Copy to `.env` (gitignored) and customize as needed.

# OpenTelemetry standard env configuration
OTEL_SERVICE_NAME=cc-proxy
OTEL_RESOURCE_ATTRIBUTES=service.name=cc-proxy,service.version=dev

# Devcontainer-local Collector (OTLP gRPC)
OTEL_EXPORTER_OTLP_ENDPOINT=http://localhost:4317
OTEL_TRACES_EXPORTER=otlp
OTEL_METRICS_EXPORTER=none

# Optional: if you later export logs via OTLP
OTEL_LOGS_EXPORTER=none

